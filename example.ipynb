   "source": [
    "import brent\n",
    "import math\n",
    "settings = brent.Settings(x_rel_tol=1e-12, x_abs_tol=1e-12, y_tol=1e-12, verbose=True)\n",
    "f = math.cos\n",
    "brent.brent(f, 0.0, 3.0, settings)"
